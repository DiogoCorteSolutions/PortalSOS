    <table id="example" class="display" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th style="text-align: center;">Linha</th>
                <th style="text-align: center;">Status</th>
                <th style="text-align: center;">Produto</th>
                <th style="text-align: center;">Atualizado</th>
                <th style="text-align: center;">Minimo</th>
                <th style="text-align: center;">Lote</th>
                <th style="text-align: center;">Ações</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr style="text-align: center;">
                    <td>@item.Linha</td>
                    <td>@item.Status</td>
                    <td>@item.Prod</td>
                    <td>@item.Atualizado</td>
                    <td>@item.Minimo</td>
                    <td>@item.Lote</td>
                    <td>
                        <a href="@Url.Action("edit", "produto", new { id = item.IdProduto })" data-toggle="tooltip" title="Editar"><img width="20" src="~/Content/img/edit.png" /></a>
                        <a href="@Url.Action("details", "produto", new { id = item.IdProduto })" data-toggle="tooltip" title="Visualizar"><img width="20" src="~/Content/img/view.png" /></a>
                        <a href="@Url.Action("excluir", "produto", new { id = item.IdProduto })" data-toggle="tooltip" title="Editar"><img width="20" src="~/Content/img/delete.png" /></a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
    <div class="col-md-12">
        <div class="form-group m-t-10">
            <a id="btn-all" name="btn-all" href="@Url.Action("index", "home")" cl class="btn btn-warning btn-custom waves-effect w-md m-b-5"><i class="fa fa-reply-all"></i>&ensp;Voltar</a>

        </div>
    </div>

