        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Item *</label>
                @Html.TextBoxFor(x => x.Item, new { @class = "form-control" })
            </div>
        </div>

        <div class="col-md-2">
            <div class="form-group">
                <label>Produto *</label>
                @Html.TextBoxFor(x => x.Produto, new { @class = "form-control" })
            </div>
        </div>
        <div class="col-md-2">
