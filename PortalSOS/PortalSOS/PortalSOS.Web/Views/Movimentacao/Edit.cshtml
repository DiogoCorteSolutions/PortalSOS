        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Atendente *</label>
                @Html.TextBoxFor(x => x.Atendente, new { @class = "form-control" })
            </div>
            <div class="col-md-2">
                <div class="form-group">
                    <label>Status *</label>
                    @Html.EditorFor(model => model.Status, new { @required = "required" })&ensp;
                </div>
            </div>
            <div id="alerta-div" style="display: none;"></div>

            <hr />

            <div class="col-md-12">
                <div class="form-group m-t-10">
                    <button id="btn-all-valid" name="btn-all-valid" type="submit" class="btn btn-success btn-custom waves-effect w-md m-b-5"><i class="fa fa-save"></i>&ensp;Salvar</button>
                    <a id="btn-all" name="btn-all" href="@Url.Action("index", "movimentacao")" class="btn btn-warning btn-custom waves-effect w-md m-b-5"><i class="fa fa-reply-all"></i>&ensp;Voltar</a>

