@model PortalSOS.Web.Models.ContasAPagar_Models

@{
    ViewBag.Title = "Visualizar Contas A Pagar";
}
<h2>@ViewBag.Title</h2>

<blockquote>
    <p>@ViewBag.Title</p>
    <small>Bem <cite title="Source Title">Vindo!!!</cite></small>
</blockquote>

@using (Html.BeginForm("details", "contasAPagar", FormMethod.Get, new { @class = "form-horizontal" }))
{
    <fieldset>
        <legend>Visualizar</legend>

        <div class="col-md-2">
            <div class="form-group">
                <label>IdLoja *</label>
                @Html.TextBoxFor(x => x.IdLoja, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>IdContasAPagar *</label>
                @Html.TextBoxFor(x => x.IdContasAPagar, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Boleto *</label>
                @Html.TextBoxFor(x => x.Boleto, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Data *</label>
                @Html.TextBoxFor(x => x.Data, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Faturado *</label>
                @Html.TextBoxFor(x => x.Faturado, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Fornecedor *</label>
                @Html.TextBoxFor(x => x.Fornecedor, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Identificacao *</label>
                @Html.TextBoxFor(x => x.Identificacao, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>DF *</label>
                @Html.TextBoxFor(x => x.Nf, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Nfatura *</label>
                @Html.TextBoxFor(x => x.Nfatura, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Nota *</label>
                @Html.TextBoxFor(x => x.Nota, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>

        <div class="col-md-2">
            <div class="form-group">
                <label>Parcela *</label>
                @Html.TextBoxFor(x => x.Parcela, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Status *</label>
                @Html.TextBoxFor(x => x.Status, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>valor *</label>
                @Html.TextBoxFor(x => x.valor, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group">
                <label>Vencimento *</label>
                @Html.TextBoxFor(x => x.Vencimento, new { @class = "form-control", @disabled = "disabled" })
            </div>
        </div>
        <hr>
        <div class="col-md-12">
            <div class="form-group m-t-10">
                <a id="btn-all" name="btn-all" href="@Url.Action("index", "contasAPagar")" cl class="btn btn-warning btn-custom waves-effect w-md m-b-5"><i class="fa fa-reply-all"></i>&ensp;Voltar</a>

            </div>
        </div>
    </fieldset>
}
