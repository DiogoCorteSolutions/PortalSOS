@model IEnumerable<PortalSOS.Web.Models.Loja_Models>
@{
    ViewBag.Title = "Exclusão d";
}
<h2>@ViewBag.Title</h2>


<blockquote>
    <p>@ViewBag.Title</p>
    <small>Bem <cite title="Source Title">Vindo</cite></small>
</blockquote>
<hr />

@if (TempData["msgerror"] != null)
{
    <div class="alert alert-dismissible alert-danger">
        <button type="button" class="close" data-dismiss="alert"></button>
        @TempData["msgerror"]
    </div>
}
else if (TempData["msgsucesso"] != null)
{
    <div class="alert alert-dismissible alert-sucess">
        <button type="button" class="close" data-dismiss="alert"></button>
        @TempData["msgsucesso"]
    </div>

}